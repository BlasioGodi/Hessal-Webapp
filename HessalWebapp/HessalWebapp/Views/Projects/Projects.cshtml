@page "/Projects"
@model HessalWebapp.Controllers.ProjectsModel
@{
    ViewData["Title"] = "Our Projects";
}

@section MetaTags {
    <meta name="description"
          content="We are a team of LPG specialists with diverse knowledge on the design, installation and maintenance of LPG systems and appliances. Connect with us for more!">
    <meta property="og:title" content="Our Projects">
    <meta property="og:description"
          content="We have delivered innovative projects through the years geared towards safety, energy conservation and sustainability.">
    <meta name="keywords"
          content="LPG Design, LPG Installation, LPG Maintenance, LPG Appliances, Fire Place Installers, LPG Installers, Fire Place Installations, LPG Safety, LPG Leak Detection, LPG Benefits, LPG Uses, LPG Production, LPG Fires, LPG Tanks, LPG Earthing">
    <meta property="og:image" content="https://hessal-sol.com/lib/src/gallery/P-03.webp">
    <meta property="og:image:width" content="1280">
    <meta property="og:image:height" content="720">
}

@Html.Partial("_SharedHeader")

<section id="projects_two">
    <div class="container section-projects">
        <div class="options mt-lg-4 mt-md-2 mt-sm-2 mt-2 mb-lg-4 mb-md-2 mb-sm-2 ">
            <ul id="project-filter">
                <li><a data-filter="*">All</a></li>
                <li><a data-filter=".residential">Residentials</a></li>
                <li><a data-filter=".commercial">Commercials</a></li>
                <li><a data-filter=".industrial">Industrial</a></li>
                <li><a data-filter=".hospital">Hospitals</a></li>
                <li><a data-filter=".hotel">Hotels</a></li>
            </ul>
        </div>
        <div class="row portfolio-grid text-style projects-gallery">
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 grid-item project project-light residential" data-category="residential">
                <div class="portfolio-item">
                    <div class="pt-img"><a href="~/lib/src/gallery/P-01.webp" class="image" title="Project - Emergency Gas Shut-off System"><img src="~/lib/src/gallery/P-01_1.webp" alt="Emergency Gas Shut-off"></a></div>
                    <div class="item-info">
                        <span class="item-info-title">Residentials</span>
                        <h3>Project - Emergency Gas Shut-off System</h3>
                    </div>
                </div>
            </div>
            <div class="col-lg-8 col-md-8 col-sm-6 col-xs-12 grid-item project industrial" data-category="industrial">
                <div class="portfolio-item">
                    <div class="pt-img"><a href="~/lib/src/gallery/P-03.webp" class="image" title="Project - Bulk Gas Tank Installation"><img src="~/lib/src/gallery/P-03_3.webp" alt="Bulk Tank Installation"></a></div>
                    <div class="item-info">
                        <span class="item-info-title">Industrial</span>
                        <h3>Project - Bulk Gas Tank Installation</h3>
                    </div>
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12 grid-item project commercial" data-category="commercial">
                <div class="portfolio-item">
                    <div class="pt-img"><a href="~/lib/src/gallery/P-04.webp" class="image" title="Project - Gas Tank Inspection"><img src="~/lib/src/gallery/P-04_4.webp" alt="Bulk Tank Inspection"></a></div>
                    <div class="item-info">
                        <span class="item-info-title">Commercials</span>
                        <h3>Project - Gas Tank Inspection</h3>
                    </div>
                </div>
            </div>
            <div class="col-lg-8 col-md-8 col-sm-6 col-xs-12 grid-item project residential" data-category="residential">
                <div class="portfolio-item">
                    <div class="pt-img"><a href="~/lib/src/gallery/P-07.webp" class="image" title="Project - Artificial LPG Fireplace"><img src="~/lib/src/gallery/P-07_7.webp" alt="Fire Place Installation"></a></div>
                    <div class="item-info">
                        <span class="item-info-title">Residentials</span>
                        <h3>
                            Project - Artificial LPG Fire-place
                        </h3>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 grid-item project hotel" data-category="hotel">
                <div class="portfolio-item">
                    <div class="pt-img"><a href="~/lib/src/gallery/P-02.webp" class="image" title="Project - Kitchen Equipment Maintenance"><img src="~/lib/src/gallery/P-02_2.webp" alt="Cooker Maintenance"></a></div>
                    <div class="item-info">
                        <span class="item-info-title">Hotels</span>
                        <h3>Project - Kitchen Equipment Maintenance</h3>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 grid-item project hospital" data-category="hospital">
                <div class="portfolio-item">
                    <div class="pt-img"><a href="~/lib/src/gallery/P-06.webp" class="image" title="Project - Kitchen Pipework Installation"><img src="~/lib/src/gallery/P-06_6.webp" alt="Pipework Installation"></a></div>
                    <div class="item-info">
                        <span class="item-info-title">Hospitals</span>
                        <h3>Project - Kitchen Pipework Installation</h3>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 grid-item project industrial" data-category="industrial">
                <div class="portfolio-item">
                    <div class="pt-img"><a href="~/lib/src/gallery/P-08.webp" class="image" title="Project - LPG Vaporizer Installation"><img src="~/lib/src/gallery/P-08_8.webp" alt="Gas Vaporizer"></a></div>
                    <div class="item-info">
                        <span class="item-info-title">Industrial</span>
                        <h3>
                            Project - LPG Vaporizer Installation
                        </h3>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 grid-item project commercial" data-category="commercial">
                <div class="portfolio-item">
                    <div class="pt-img"><a href="~/lib/src/gallery/P-09.webp" class="image" title="Project - LPG Reticulation Pipework"><img src="~/lib/src/gallery/P-09_9.webp" alt="Pipework Installation"></a></div>
                    <div class="item-info">
                        <span class="item-info-title">Commercials</span>
                        <h3>Project - LPG Reticulation Pipework</h3>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 grid-item project residential" data-category="residential">
                <div class="portfolio-item">
                    <div class="pt-img">
                        <a href="~/lib/src/gallery/P-10.webp" class="image" title="Project - Solenoid Valve Installation"><img src="~/lib/src/gallery/P-10_10.webp" alt="Solenoid Valve"></a>
                    </div>
                    <div class="item-info">
                        <span class="item-info-title">Residentials</span>
                        <h3>Project - Solenoid Valve Installation</h3>
                    </div>
                </div>
            </div>
            <div class="col-lg-8 col-md-8 col-sm-6 col-xs-12 grid-item project commercial" data-category="commercial">
                <div class="portfolio-item">
                    <div class="pt-img"><a href="~/lib/src/gallery/P-05.webp" class="image" title="Project - LPG Cylinder Installation"><img src="~/lib/src/gallery/P-05_5.webp" alt="Gas Cylinder Installation"></a></div>
                    <div class="item-info">
                        <span class="item-info-title">Commercial</span>
                        <h3>Project - LPG Cylinder Installation</h3>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="load-more-items" id="load-more">
        <button class="btn btn-primary" onclick="" title="">Load more</button>
    </div><!--load-more-items end-->
</section>

@section Scripts{
    <script src="~/lib/node_modules/owl.carousel/dist/owl.carousel.min.js"></script>
    <script src="~/js/wow.min.js"></script>
    <script src="~/js/jquery.stellar.min.js"></script>
    <script src="~/lib/jquery/dist/jquery.magnific-popup.js"></script>
    <script src="~/js/owl-carousel-project.js"></script>
    <script src="~/js/interface.js"></script>
    <script src="~/lib/node_modules/isotope-layout/dist/isotope.pkgd.min.js"></script>
}
